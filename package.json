{
  "name": "@sveltestrap/sveltestrap",
  "version": "1.0.0",
  "main": "dist/sveltestrap.js",
  "module": "dist/sveltestrap.es.js",
  "jsnext:main": "dist/sveltestrap.es.js",
  "svelte": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "keywords": [
    "svelte",
    "components",
    "sveltestrap",
    "bootstrap",
    "javascript",
    "accordian",
    "alerts",
    "badges",
    "breadcrumbs",
    "buttons",
    "cards",
    "dropdowns",
    "forms",
    "icons",
    "inputs",
    "lists",
    "modals",
    "notifications",
    "pagination",
    "popovers",
    "portals",
    "progress",
    "spinners",
    "tabs",
    "tables",
    "toasts",
    "tooltips"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:sveltestrap/sveltestrap.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "svelte": "./src/index.js",
      "require": "./dist/index.js",
      "module": "./dist/index.ejs",
      "import": "./dist/index.ejs"
    }
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "dist": "vite build",
    "docs:build": "storybook build -o docs && cp ./.storybook/docs/* ./docs",
    "docs:deploy": "pnpm run docs:build && npx gh-pages -d docs",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint -c ./.eslintrc.json --fix \"src/**/*.{.js,svelte}\"",
    "format": "prettier --write \"src/**/*.{js,json,svelte}\"",
    "test": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage && open ./coverage/index.html"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addons": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/svelte": "^7.5.2",
    "@storybook/svelte-webpack5": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.4.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@tsconfig/svelte": "^2.0.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^9.8.6",
    "eslint": "^8.54.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-vitest": "^0.3.10",
    "husky": "^6.0.0",
    "jsdom": "^22.1.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.4",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "prismjs": "^1.25.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.2",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "^5.1.1",
    "typescript": "^4.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  },
  "browserslist": "last 2 versions"
}
